    "@resolve-js/eventstore-postgresql": "0.28.1-alpha",
    "@resolve-js/eventstore-postgresql-serverless": "0.28.1-alpha",
    "@resolve-js/readmodel-lite": "0.28.1-alpha",
    "@resolve-js/readmodel-mysql": "0.28.1-alpha",
    "@resolve-js/readmodel-postgresql-serverless": "0.28.1-alpha",
    "@resolve-js/scripts": "0.28.1-alpha",
    "@resolve-js/testing-tools": "0.28.1-alpha",
    "aws-sdk": "2.860.0",
    "jest": "26.6.3",
